create table sma_file
(
sma00       varchar2(1) NOT NULL,    /*KEY VALUE(0)                           */
                                     /*KEY VALUE(0)                         SYS*/
sma01       varchar2(1),             /*当前 Tiptop 系统是否可开放使           */
                                     /*目前 Tiptop 系統是否可開放使用。     SYS*/
                                     /*將會影響使用者可執行作業之功能及範圍   */
                                     /*正確值 Y/N                             */
                                     /*Y: 正常使用                            */
                                     /*N: 僅供使用者列印、查詢                */
