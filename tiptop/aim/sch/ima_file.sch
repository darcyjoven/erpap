create table ima_file
(
ima01       varchar2(40) NOT NULL,   /*料件编号                               */
                                     /*料件編號                               */
                                     /*料件存在系統中代表編號, 是唯一的       */
ima02       varchar2(120),           /*品名                                   */
                                     /*品名規格                               */
                                     /*描述該料件的品名規格, 如有需要進一步描 */
                                     /*述, 則可利用品名規格額外說明資料檔記錄 */
ima021      varchar2(120),           /*规格                                   */
                                     /*Specification                  (97/08/18*/
ima03       varchar2(40),            /*目录参考号码                           */
                                     /*目錄參考號碼                           */
                                     /*料件所屬的目錄編號                     */
                                     /*使用者可依目錄編號歸類管理列印         */
                                     /*預設值 依分群碼類別預設 或 空白        */
ima04       varchar2(255),           /*工程图号                               */
                                     /*工程圖號                               */
                                     /*該料件所在的主要工程圖號               */
                                     /*可提供給生產/工程部門參考              */
                                     /*預設值 依分群碼類別預設 或 空白        */
ima05       varchar2(10),            /*当前使用版本                           */
                                     /*目前使用版本                           */
                                     /*本料件目前使用的版本編號               */
                                     /*最近工程變異版本                       */
ima06       varchar2(10),            /*分群码                                 */
                                     /*分群碼                                 */
                                     /*0: 表由其他相關系統而來尚未分群        */
                                     /*當在料件基本資料建立時, 可利用分群碼來 */
                                     /*預設 相關欄位基本預設值                */
ima07       varchar2(1),             /*ABC码                                  */
                                     /*ABC 碼                                 */
                                     /*對料件庫存作價值特性分類管理, 將會影響 */
                                     /*料件的週期盤點策略.                    */
                                     /*可透過 ABC碼計算作業 依系統建議而得    */
                                     /*A類料件應為最高價值, B類料件應為次高價 */
                                     /*值, 而C類料件應為較低價值              */
                                     /*預設值 依分群碼類別預設 或 空白        */
ima08       varchar2(1),             /*来源码                                 */
                                     /*來源碼                                 */
                                     /*說明料件屬性, 用以勾劃出不同歸屬特性的 */
                                     /*料件, 以便在料件庫存異動及物料需求 ..  */
                                     /*等作業時, 提供作業管制方式或限制       */
                                     /*正確值 C/T/D/A/M/P/X/K/U/V/R/Z/S       */
                                     /*預設值 依分群碼類別預設 或 空白, 且結果*/
                                     /*需有一種來源碼                         */
                                     /*C: 規格組件 (Configurable Product)     */
                                     /*T: 最後規格料件 (FAS Product)          */
                                     /*D: 特性料件 (Feature)                  */
                                     /*A: 族群料件 (Family Product)           */
                                     /*M: 自製料件 (Make Product)             */
                                     /*P: 採購料件 (Purchase)                 */
                                     /*X: 虛擬料件 (Phantom)                  */
                                     /*K: 配件虛擬料件 (Kit)                  */
                                     /*U: 自製大宗料件                        */
                                     /*V: 採購大宗料件                        */
                                     /*R: 在製途料件 (Routable)               */
                                     /*Z: 雜項料件 (Miscellaneous)            */
                                     /*S: 廠外加工料件 (Subcontract)          */
ima09       varchar2(10),            /*其他分群码 一                          */
                                     /*其他分群碼 一                          */
                                     /*提供給使用者, 對料件分群/分類除在分群  */
                                     /*碼中定義外, 可提供給其它的定義方式, 以 */
                                     /*供 管理/匯集報表 使用                  */
                                     /*將可分成 一/二/三/四 四個欄位以供交互  */
                                     /*定義使用                               */
                                     /*使用者可自行定義                       */
                                     /*預設值 依分群碼類別預設 或 空白        */
ima10       varchar2(10),            /*其他分群码 二                          */
                                     /*其他分群碼 二                          */
                                     /*提供給使用者, 對料件分群/分類除在分群  */
                                     /*碼中定義外, 可提供給其它的定義方式, 以 */
                                     /*供 管理/匯集報表 使用                  */
                                     /*將可分成 一/二/三/四 四個欄位以供交互  */
                                     /*定義使用                               */
                                     /*使用者可自行定義                       */
                                     /*預設值 依分群碼類別預設 或 空白        */
ima11       varchar2(10),            /*其他分群码 三                          */
                                     /*其他分群碼 三                          */
                                     /*提供給使用者, 對料件分群/分類除在分群  */
                                     /*碼中定義外, 可提供給其它的定義方式, 以 */
                                     /*供 管理/匯集報表 使用                  */
                                     /*將可分成 一/二/三/四 四個欄位以供交互  */
                                     /*定義使用                               */
                                     /*使用者可自行定義                       */
                                     /*預設值 依分群碼類別預設 或 空白        */
ima12       varchar2(10),            /*其他分群码 四                          */
                                     /*成本分群碼                             */
ima13       varchar2(40),            /*规格组件料件编号                       */
                                     /*規格組件料件編號                       */
                                     /*如果料件來源碼為'T'(最後規格料件), 必  */
                                     /*須儲存其規格組件料件編號               */
                                     /*否則不可輸入, 應為空白                 */
                                     /*需存在料件資料主檔中, 且其料件來源碼需 */
                                     /*為'C'(規格組件)                        */
ima14       varchar2(1),             /*是否为工程料件                         */
                                     /*是否為工程料件                         */
                                     /*闡明該料件是否為工程料件, 以便分別管理 */
                                     /*工程料件及非工程料件                   */
                                     /*正確值 'Y' 或 'N'                      */
                                     /*Y: 工程料件                            */
                                     /*N: 非工程料件                          */
                                     /*預設值 依分群碼類別預設 或 'N'         */
ima15       varchar2(1),             /*保税与否                               */
                                     /*保稅與否                               */
                                     /*闡明該料件是否為保稅料件, 以便作 保稅  */
                                     /*處理作業                               */
                                     /*正確值 'Y' 或 'N'                      */
                                     /*Y: 保稅料件                            */
                                     /*N: 非保稅料件                          */
                                     /*預設值 依分群碼類別預設 或 'N'         */
ima16       number(5),               /*低阶码                                 */
                                     /*低階碼                                 */
                                     /*每一料件應皆有其低階碼, 如果為產品結構 */
                                     /*中的最上階, 其低階碼將被系統設定為 '0' */
                                     /*, 而非在產品結構中最上階者, 其階數將按 */
                                     /*其由最上階往下累計最大者為其低階碼; 其 */
                                     /*它無產品結構者, 其階數亦將被設定為 '0' */
                                     /*新增料件設定初始值為 '99', 日後可透過  */
                                     /*低階碼計算作業 而得其所在階數          */
                                     /*系統維護                               */
ima17       varchar2(4),             /*No Use                                 */
                                     /*NO USE                                 */
ima17_fac   number(16,8),            /*No Use                                 */
ima18       number(15,3),            /*单位重量                               */
                                     /*單位淨重 (Kg)                          */
ima19       varchar2(4),             /*保税料件进出口分类统计用类             */
                                     /*保稅料件進出口分類統計用類別           */
ima20       number(9,3),             /*保税料件年度盘差容许率                 */
                                     /*保稅料件年度盤差容許率                 */
ima21       varchar2(20),            /*保税料件税则编号                       */
                                     /*保稅料件稅則編號                       */
ima22       number(9,3),             /*保税料件应补税税率                     */
                                     /*保稅料件應補稅稅率                     */
ima23       varchar2(10),            /*仓管员                                 */
                                     /*倉管員                                 */
                                     /*該料件歸屬倉庫管理員                   */
                                     /*提供料件收/發料等, 庫存異動作業/單據管 */
                                     /*制, 列印依據                           */
                                     /*如果有設定倉管員, 則需存在 員工檔 內   */
                                     /*預設值 依分群碼類別預設 或 空白        */
ima24       varchar2(1),             /*检验码                                 */
                                     /*檢驗否                                 */
ima25       varchar2(4),             /*库存单位                               */
                                     /*庫存單位                               */
                                     /*料件庫存數量計算單位, 在多單位管理系統 */
                                     /*下, 此庫位單位亦為系統換算基準單位     */
                                     /*預設值 依分群碼類別預設 或 空白        */
                                     /*必須輸入一單位, 且需存在 單位檔 內     */
                                     /*在以下各所有文件說明中, 稱之'料件庫存  */
                                     /*單位'                                  */
ima26       number(15,3),            /*No Use                                 */
                                     /*MPS/MRP 可用庫存數量(可用)             */
                                     /*儲存該料件可用的庫存數量, 使用料件庫存 */
                                     /*單位                                   */
                                     /*採用單一倉儲系統時, 此欄位值即為可用庫 */
                                     /*存明細數量, 其數量單位即為唯一的料件庫 */
                                     /*存單位, 可人為直接輸入                 */
                                     /*採用多倉儲系統時, 此欄位值即為所有可用 */
                                     /*庫存明細數量, 其庫存單位以料件庫存單位 */
                                     /*為基準下的可用庫存數量總和, 此欄位此時 */
                                     /*由將系統維護, 不提供人為修改           */
                                     /*新增時, 初始值設定為 '0'               */
                                     /*在以下各所有文件說明中, 稱之'料件庫存  */
                                     /*數量' 或 '可用料件庫存數量'            */
ima261      number(15,3),            /*No Use                                 */
                                     /*不可用庫存數量                         */
                                     /*儲存該料件不可用的庫存數量, 使用料件庫 */
                                     /*存單位                                 */
                                     /*採用單一倉儲系統時, 此欄位值即為不可用 */
                                     /*庫存明細數量, 其數量單位即為唯一的料件 */
                                     /*庫存單位, 可人為直接輸入               */
                                     /*採用多倉儲系統時, 此欄位值即為所有不可 */
                                     /*用庫存明細數量, 其庫存單位以料件庫存單 */
                                     /*位為基準下的不可用庫存數量總和, 此欄位 */
                                     /*此時將由系統維護, 不提供人為修改       */
                                     /*新增時, 初始值設定為 '0'               */
                                     /*在以下各所有文件說明中, 稱之'料件不可  */
                                     /*用料件庫存數量'                        */
ima262      number(15,3),            /*No Use                                 */
                                     /*庫存可用數量                           */
ima27       number(15,3),            /*安全库存量                             */
                                     /*安全庫存量                             */
                                     /*當在使用上, 希望在考量 MPS/MRP 產生計  */
                                     /*劃訂單(PLANNED ORDER)時, 在需求數量上  */
                                     /*能加上對庫存數量亦能維持一定的水準的庫 */
                                     /*存策略, 則可使用安全庫存量 或 安全庫存 */
                                     /*期間的作法                             */
                                     /*安全庫存量是以一固定數量來達成上述庫存 */
                                     /*策略                                   */
                                     /*預設值 依分群碼類別預設 或 '0'         */
ima271      number(15,3),            /*最高储存数量                           */
                                     /*最高儲存數量                           */
ima28       number(15,3),            /*安全库存期间                           */
                                     /*安全庫存期間                           */
                                     /*當在使用上, 希望在考量 MPS/MPR 產生計  */
                                     /*劃訂單(PLANNED ORDER)時, 在需求數量上  */
                                     /*能加上對庫存數量亦能維持一定的水準的庫 */
                                     /*存策略, 則可使用安全庫存量 或 安全庫存 */
                                     /*期間的作法                             */
                                     /*安全庫存期間是以一日期間需求數量來達成 */
                                     /*上述庫存策略                           */
                                     /*預設值 依分群碼類別預設 或 '0'         */
ima29       date,                    /*最近异动日期                           */
                                     /*最近異動日期                           */
                                     /*該料件最近一次庫存異動日期, 可能為     */
                                     /*入庫/出庫/盤點日期                     */
                                     /*系統維護                               */
ima30       date,                    /*最近盘点日期                           */
                                     /*最近盤點日期                           */
                                     /*該料件最近一次庫存週期盤點日期         */
                                     /*系統維護                               */
ima31       varchar2(4),             /*销售单位                               */
                                     /*銷售單位                               */
                                     /*該料件的預設銷售單位, 而且在作銷售數量 */
                                     /*統計的作為基準單位                     */
                                     /*在以下各所有文件說明中, 稱之'料件銷售  */
                                     /*單位'                                  */
                                     /*預設值 依分群碼類別預設 或 料件庫存單位*/
                                     /*如果不為可銷售料件則可不輸入; 假如, 輸 */
                                     /*入之單位不為料件庫存單位時, 必須有單位 */
                                     /*間的轉換率存在, 且將儲存於下一欄位(銷售*/
                                     /*單位/庫存單位換算率)                   */
ima31_fac   number(20,8),            /*销售单位/库存单位换算率                */
                                     /*Sales Unit/Inventory Unit Conversion RateConv. Rate Of Item Sales Unit VS Item Inventory UnitDefault Value  Default By Group Code : 0 Or Settiong Conv. Rate Between Units*/
ima32       number(20,6),            /*标准销售订价                           */
                                     /*標準售價                               */
ima33       number(20,6),            /*最近销售单价                           */
                                     /*最近售價                               */
ima34       varchar2(10),            /*成本中心                               */
                                     /*儲存該料件成本計算時, 所屬的成本中心   */
                                     /*當 製造管理系統參數 ASM #023 = 1 時,   */
                                     /*則可依需要去設定料件歸屬成本中心, 如此 */
                                     /*便可取得計算出該料件的人工成本及製造費 */
                                     /*用, 如果未設定則料件成本要素中, 除材料 */
                                     /*成本要素外, 其餘皆不動                 */
                                     /*當 製造管理系統參數 ASM #023 = 2 時,   */
                                     /*將使用 工作站及製程 來取得計算出該料件 */
                                     /*的人工成本及製造費用, 所以不需設定成本 */
                                     /*中心, 此時此欄位無用                   */
                                     /*當 製造管理系統參數 ASM #023 = 3 時,   */
                                     /*則可依需要去設定料件歸屬成本中心, 如此 */
                                     /*便可取得計算出該料件的人工成本及製造費 */
                                     /*用(由成本中心資料), 如果未設定則料件將 */
                                     /*使用 工作站及製程 來取得計算出該料件的 */
                                     /*人工成本及製造費用                     */
                                     /*預設值 依分群碼類別預設 或 '空白'      */
                                     /*若有設定 成本中心 時, 則成本中心需存在 */
                                     /*成本中心主檔內                         */
ima35       varchar2(10),            /*主要仓库                               */
                                     /*主要倉庫別                             */
                                     /*儲存該料件存放的主要倉庫編號           */
                                     /*當採用單一倉儲系統時, 本欄位不輸入     */
                                     /*當採用多倉儲系統時, 本欄位可輸入, 或由 */
                                     /*系統維護 或不輸入, 但其倉庫編號需存在  */
                                     /*倉庫資料主檔內                         */
                                     /*預設值 依分群碼類別預設 或 '空白'      */
ima36       varchar2(10),            /*主要库位别                             */
                                     /*主要儲位別                             */
                                     /*儲存該料件存放的主要倉庫別中的儲位     */
                                     /*當採用單一倉儲系統時, 本欄位不輸入     */
                                     /*當採用多倉儲系統時, 本欄位可輸入, 或由 */
                                     /*系統維護 或不輸入, 但其倉庫編號/儲位   */
                                     /*需存在倉庫/存放位置資料主檔內          */
                                     /*預設值 依分群碼類別預設 或 '空白'      */
ima37       varchar2(1),             /*补货策略码                             */
                                     /*補貨策略碼                             */
                                     /*對該料件在執行 MPS/MPR 時的計劃訂單/訂 */
                                     /*單數量的建議產生策略                   */
                                     /*正確值 0/1/2/3/4/5                     */
                                     /*0: 再訂購點    Reorder point parts     */
                                     /*1: MPS         MPS parts               */
                                     /*2: MRP         MRP parts               */
                                     /*3: FAS         FAS parts               */
                                     /*4: 訂單式生產  On-order parts          */
                                     /*5: 期間採購    Fixed interval parts    */
                                     /*採用 再訂購點   時, 則會依庫存數量低於 */
                                     /*                    再補貨點數量時, 產 */
                                     /*                    生計劃訂單         */
                                     /*採用 MPS        時, 則會依主生產排程產 */
                                     /*                    生計劃訂單         */
                                     /*採用 MRP        時, 則會依物料需求計劃 */
                                     /*                    產生計劃訂單       */
                                     /*採用 FAS        時, 則會依主生產排程產 */
                                     /*                    生計劃訂單         */
                                     /*採用 訂單式生產 時, 則會依物料需求計劃 */
                                     /*                    產生計劃訂單, 而訂 */
                                     /*                    購數量以滿足客戶訂 */
                                     /*                    單需求即可         */
                                     /*                    目前未使用         */
                                     /*採用 期間採購   時, 則會固定的採購期間 */
                                     /*                    固定的採購數量來產 */
                                     /*                    生計劃訂單         */
                                     /*預設值 依分群碼類別預設 或 '空白'      */
                                     /*必須有一正確值                         */
ima38       number(15,3),            /*再补货点                               */
                                     /*再補貨點                               */
                                     /*儲存當補貨策略採用再訂購點方式時的再補 */
                                     /*貨點水準數量                           */
                                     /*預設值 依分群碼類別預設 或 '0'         */
                                     /*其值為'0'時, 表示使用最小生產或採購數量*/
ima39       varchar2(24),            /*料件所属会计科目                       */
                                     /*料件所屬會計科目                       */
                                     /*儲存該料件成本會計所歸屬的會計科目     */
                                     /*輸入之檢查方式, 按製造管理系統參數來檢 */
                                     /*查合法與否                             */
                                     /*預設值 依分群碼類別預設 或 '空白'      */
ima40       number(15,3),            /*累计使用数量  期间                     */
                                     /*累計使用數量  期間                     */
                                     /*儲存該料件目前期間/月份使用的累計數量  */
                                     /*祇有出庫時會修改此數量                 */
                                     /*系統維護                               */
ima41       number(15,3),            /*累计使用数量  年度                     */
                                     /*累計使用數量  年度                     */
                                     /*儲存該料件目前年度(含 目前期間累計數   */
                                     /*量)使用的累計數量                      */
                                     /*祇有出庫時會修改此數量                 */
                                     /*系統維護                               */
ima42       varchar2(1),             /*批号追踪方式                           */
                                     /*批號追蹤方式                           */
                                     /*正確值 0/1/2                           */
                                     /*0: 無                                  */
                                     /*1: 軟性                                */
                                     /*2: 硬性                                */
